{
  "name": "@axelarjs/maestro",
  "version": "0.2.1",
  "private": true,
  "publishConfig": {
    "access": "restricted"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axelarnetwork/axelarjs"
  },
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "scripts/build.sh",
    "build:analyze": "ANALYZE=true pnpm build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "dev:e2e": "concurrently anvil \"NEXT_PUBLIC_E2E_ENABLED=true pnpm dev\"",
    "test:e2e": "dotenv -e .env.local npx playwright test",
    "ci:e2e": "npx playwright test",
    "test": "vitest --run",
    "test:coverage": "vitest --coverage --run",
    "sync": "tsx scripts/sync.ts",
    "codegen": "pnpm wagmi generate && tsx scripts/postcodegen.ts",
    "codegen:env": "tsx scripts/env.ts",
    "drizzle:generate": "pnpm drizzle-kit generate:pg",
    "drizzle:up": "pnpm drizzle-kit up:pg",
    "drizzle:push": "pnpm drizzle-kit push:pg",
    "drizzle:studio": "pnpm drizzle-kit studio"
  },
  "dependencies": {
    "@axelar-network/axelarjs-sdk": "^0.13.6",
    "@axelarjs/api": "workspace:*",
    "@axelarjs/core": "workspace:*",
    "@axelarjs/evm": "workspace:*",
    "@axelarjs/ui": "workspace:*",
    "@axelarjs/utils": "workspace:*",
    "@hookform/resolvers": "^3.3.2",
    "@sentry/nextjs": "7.64.0",
    "@tanstack/react-query": "4.33.0",
    "@trpc/client": "^10.40.0",
    "@trpc/next": "^10.40.0",
    "@trpc/react-query": "^10.40.0",
    "@trpc/server": "^10.40.0",
    "@vercel/kv": "^0.2.3",
    "@vercel/postgres": "^0.4.2",
    "@web3modal/ethereum": "^2.7.1",
    "@web3modal/react": "^2.7.1",
    "drizzle-orm": "^0.28.6",
    "lucide-react": "^0.265.0",
    "next": "^13.5.5",
    "nextjs-cors": "^2.1.2",
    "nprogress": "^0.2.0",
    "pg": "^8.11.3",
    "rambda": "^7.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-jazzicon": "^1.0.4",
    "react-spinners": "^0.13.8",
    "superjson": "^1.13.3",
    "swagger-ui-react": "~5.7.2",
    "tailwind-styled-components": "^2.2.0",
    "trpc-openapi": "^1.2.0",
    "typescript": "^5.2.2",
    "unfetch": "^4.2.0",
    "viem": "~1.14.0",
    "wagmi": "1.4.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@next/bundle-analyzer": "^13.5.5",
    "@playwright/test": "^1.39.0",
    "@sentry/cli": "^2.21.2",
    "@tanstack/react-query-devtools": "4.33.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.8.6",
    "@types/react": "18.2.21",
    "@types/react-dom": "^18.2.13",
    "@types/nprogress": "^0.2.1",
    "@types/swagger-ui-react": "^4.18.1",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.1.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@wagmi/cli": "^1.5.2",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.1",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "drizzle-kit": "^0.19.13",
    "drizzle-zod": "^0.5.1",
    "eslint": "^8.51.0",
    "eslint-config-next": "^13.5.5",
    "happy-dom": "^9.20.3",
    "matchers": "link:@testing-library/jest-dom/matchers",
    "next-auth": "^4.23.2",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3",
    "tsx": "^3.13.0",
    "vitest": "^0.34.6",
    "zx": "^7.2.3"
  }
}
