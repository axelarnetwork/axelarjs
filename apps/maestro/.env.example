# =============================================================================
# DATABASE CONFIGURATION - POSTGRESQL (NEON)
# =============================================================================
# PostgreSQL connection URLs
POSTGRES_URL=postgresql://xx:xx@xx.xx.aws.neon.tech/neondb?sslmode=require
POSTGRES_URL_NON_POOLING=postgresql://xx:xx@xx.xx.aws.neon.tech/neondb?sslmode=require
POSTGRES_URL_NO_SSL=postgresql://xx:xx@xx-pooler.xx.aws.neon.tech/neondb
POSTGRES_PRISMA_URL=postgresql://xx:xx@xx-pooler.xx.aws.neon.tech/neondb?connect_timeout=15&sslmode=require

# PostgreSQL connection details
POSTGRES_USER=xx
POSTGRES_HOST=xx-pooler.xx.aws.neon.tech
POSTGRES_PASSWORD=xx
POSTGRES_DATABASE=neondb

# =============================================================================
# CACHE CONFIGURATION - REDIS (UPSTASH)
# =============================================================================
# Redis connection details
KV_URL="rediss://default:xx@xx.upstash.io:6379"
KV_REST_API_URL="https://xx.upstash.io"
KV_REST_API_TOKEN="xx"
KV_REST_API_READ_ONLY_TOKEN="xx"

# =============================================================================
# NETWORK & ENVIRONMENT CONFIGURATION
# =============================================================================
# Network environment (mainnet, testnet, devnet-amplifier)
NEXT_PUBLIC_NETWORK_ENV=testnet

# Site URLs and endpoints
NEXT_PUBLIC_SITE_URL=https://testnet.interchain.axelar.dev
NEXT_PUBLIC_TESTNET_URL="https://testnet.interchain.axelar.dev/"
NEXT_PUBLIC_EXPLORER_URL="https://testnet.axelarscan.io"

CONFIG_FILE_URL="https://axelar-mainnet.s3.us-east-2.amazonaws.com/configs/mainnet-config-1.x.json"

# =============================================================================
# RPC SERVER OVERRIDES
# =============================================================================
RPC_hedera_testnet=""

# =============================================================================
# WALLET & CONNECTIVITY
# =============================================================================
# WalletConnect project ID for wallet connections
NEXT_PUBLIC_WALLET_CONNECT_PROJECT_ID="MY-PROJECT-ID"
NEXT_PUBLIC_XAMAN_API_KEY="ffffffff-aaaa-bbbb-1111-123456789abc"

# Disabled chains (comma separated)
NEXT_PUBLIC_DISABLED_CHAINS="optimism"

# Excluded WalletConnect wallets (comma separated list of wallet ids)
# Wallet ids can be found at https://walletconnect.com/explorer?type=wallet
NEXT_PUBLIC_DISABLED_WALLET_IDS="wallet-id-1,wallet-id-2"

# =============================================================================
# BLOCKCHAIN & CONTRACTS
# =============================================================================
# Contract addresses
NEXT_PUBLIC_INTERCHAIN_TOKEN_FACTORY_ADDRESS="0x83a93500d23Fbc3e82B410aD07A6a9F7A0670D66"
NEXT_PUBLIC_INTERCHAIN_TOKEN_SERVICE_ADDRESS=0xB5FB4BE02232B1bBA4dC8f81dc24C26980dE9e3C

# Gas limits for transactions
NEXT_PUBLIC_INTERCHAIN_TRANSFER_GAS_LIMIT=11000000
NEXT_PUBLIC_INTERCHAIN_DEPLOYMENT_GAS_LIMIT=11000000

# Whitelisted chains for VM operations
NEXT_PUBLIC_WHITELISTED_DEST_CHAINS_FOR_VM=avalanche-fuji,optimism-sepolia,eth-sepolia,stellar-2025-q1

# =============================================================================
# STELLAR NETWORK CONFIGURATION
# =============================================================================
# Stellar network passphrase
STELLAR_NETWORK_PASSPHRASE="Test SDF Network ; September 2015"

# =============================================================================
# FEATURES & COMPETITION
# =============================================================================
# Feature flags (comma separated)
NEXT_PUBLIC_ENABLED_FEATURES=

# Competition timestamps (ISO format)
NEXT_PUBLIC_COMPETITION_START_TIMESTAMP="2024-02-08T17:00Z"
NEXT_PUBLIC_COMPETITION_END_TIMESTAMP="2024-02-15T17:00Z"

# =============================================================================
# AUTHENTICATION & SECURITY
# =============================================================================
# NextAuth configuration (can use arbitrary values for development)
NEXTAUTH_SECRET="super-secret-key"
JWT_SECRET="another-super-secret-key"

# =============================================================================
# TELEMETRY & MONITORING
# =============================================================================
# Sentry error tracking
NEXT_PUBLIC_SENTRY_DSN="sentry-dsn"
SENTRY_AUTH_TOKEN="sntrs_"

# Google Analytics & Tag Manager
NEXT_PUBLIC_GTM_ID=
NEXT_PUBLIC_GA_MEASUREMENT_ID=GTM-ABCDEFGH

# =============================================================================
# TESTING & DEVELOPMENT
# =============================================================================
# E2E testing configuration
NEXT_PUBLIC_E2E_ENABLED=false
NEXT_PUBLIC_E2E_URL=http://localhost:3000

# Development settings
DISABLE_CACHE="true"
FEE_MULTIPLIER=1

# =============================================================================
# EXTERNAL SERVICES & INTEGRATIONS
# =============================================================================
# Bug reporting
NEXT_PUBLIC_FILE_BUG_REPORT_URL=https://axelar.zendesk.com/hc/en-us/requests/new

# Axelar configurations
NEXT_PUBLIC_AXELAR_CONFIGS_URL=https://github.com/axelarnetwork/axelar-configs/blob/main/cli/wizard/commands/list-squid-token/README.md

# Git branch information
NEXT_PUBLIC_GIT_BRANCH=main

# Slack webhook for alerts
WEBHOOK_SLACK_INFO_ALERTS_ITS_CHANNEL="https://hooks.slack.com/services/XXXXX/XXXXXX/XXXXX"

# OpenAI API key
OPENAI_API_KEY="sk-"
