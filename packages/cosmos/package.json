{
  "name": "@axelarjs/cosmos",
  "version": "0.1.5",
  "publishConfig": {
    "access": "public"
  },
  "description": "Cosmos SDK for Axelar Network",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axelarnetwork/axelarjs"
  },
  "files": [
    "./build",
    "./query-client",
    "./signing-client",
    "./constants.js",
    "./utils.js",
    "./types.js",
    "./index.js"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./build/module/index.js",
      "require": "./build/commonjs/index.js"
    },
    "./*": {
      "import": "./build/module/*.js",
      "require": "./build/commonjs/*.js"
    },
    "./signing-client": {
      "import": "./build/module/signing-client/index.js",
      "require": "./build/commonjs/signing-client/index.js"
    },
    "./query-client": {
      "import": "./build/module/query-client/index.js",
      "require": "./build/commonjs/query-client/index.js"
    }
  },
  "scripts": {
    "build": "./scripts/build.sh",
    "clean": "./scripts/clean.sh",
    "compile": "tsc --noEmit",
    "test": "vitest run",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "keywords": [],
  "author": "",
  "license": "LicenseRef-LICENSE",
  "dependencies": {
    "@axelarjs/config": "workspace:*",
    "@axelarjs/proto": "workspace:*",
    "@cosmjs/proto-signing": "^0.31.0",
    "@cosmjs/stargate": "^0.31.0",
    "@cosmjs/tendermint-rpc": "^0.31.0",
    "inflection": "^2.0.1",
    "long": "^5.2.3"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^2.0.2",
    "cosmjs-types": "^0.8.0",
    "rambda": "^7.5.0",
    "rimraf": "^5.0.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.4"
  }
}
