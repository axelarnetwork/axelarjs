{
  "name": "@axelarjs/proto",
  "version": "0.35.5",
  "publishConfig": {
    "access": "public"
  },
  "description": "Axelarcore supporting protobuf types",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axelarnetwork/axelarjs"
  },
  "files": [
    "build",
    "cosmos",
    "gogoproto",
    "google",
    "axelar",
    "tendermint"
  ],
  "exports": {
    "./*": {
      "import": "./build/module/*.js",
      "require": "./build/commonjs/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./build/module/*"
      ]
    }
  },
  "scripts": {
    "clean": "./scripts/clean.sh",
    "sync": "./scripts/sync.sh",
    "codegen": "./scripts/codegen.sh",
    "build": "./scripts/build.sh",
    "compile": "tsc --noEmit",
    "prepublish": "pnpm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "docgen": "typedoc",
    "deploy:docs": "gh-pages -d docs -e proto"
  },
  "license": "LicenseRef-LICENSE",
  "dependencies": {
    "long": "^5.2.3",
    "protobufjs": "^7.2.6"
  },
  "devDependencies": {
    "@axelarjs/config": "workspace:*",
    "@types/node": "^20.11.28",
    "degit": "^2.8.4",
    "gh-pages": "^6.1.1",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-proto": "^1.169.1",
    "typedoc": "^0.25.12",
    "typescript": "^5.4.2"
  }
}
