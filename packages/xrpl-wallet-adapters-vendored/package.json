    "@transia/xrpl": "4.4.4-alpha.1",
    "@wallet-standard/core": "^1.1.1",
    "@walletconnect/modal": "^2.7.0",
    "@walletconnect/types": "^2.22.4",
    "@walletconnect/universal-provider": "^2.22.4",
