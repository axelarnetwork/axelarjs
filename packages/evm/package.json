{
  "name": "@axelarjs/evm",
  "version": "0.1.1",
  "description": "EVM clients & utils",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axelarnetwork/axelarjs"
  },
  "files": [
    "./build",
    "./contracts",
    "./index.js",
    "./clients.js"
  ],
  "exports": {
    ".": {
      "import": "./build/module/index.js",
      "require": "./build/commonjs/index.js"
    },
    "./*": {
      "import": "./build/module/*.js",
      "require": "./build/commonjs/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./build/module/*"
      ]
    }
  },
  "scripts": {
    "build": "./scripts/build.sh",
    "clean": "./scripts/clean.sh",
    "compile": "tsc --noEmit",
    "postinstall": "node scripts/postinstall.cjs"
  },
  "keywords": [],
  "author": "",
  "license": "LicenseRef-LICENSE",
  "dependencies": {
    "viem": "^1.2.9"
  },
  "devDependencies": {
    "@axelar-network/interchain-token-service": "^0.1.0",
    "@axelarjs/config": "workspace:*",
    "chalk": "^4.1.2",
    "package-json": "^7.0.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "strip-ansi": "^6.0.1",
    "typescript": "^5.1.6"
  }
}
