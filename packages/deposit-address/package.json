{
  "name": "@axelarjs/deposit-address",
  "version": "0.1.24",
  "description": "Axelarjs Deposit Address",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axelarnetwork/axelarjs"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "./build",
    "./index.js",
    "./index.d.ts",
    "./browser.js",
    "./browser.d.ts",
    "./node.js",
    "./node.d.ts",
    "./types.js",
    "./types.d.ts",
    "./get-deposit-address"
  ],
  "exports": {
    ".": {
      "import": "./build/module/index.js",
      "require": "./build/commonjs/index.js"
    },
    "./*": {
      "import": "./build/module/*.js",
      "require": "./build/commonjs/*.js"
    }
  },
  "scripts": {
    "build": "./scripts/build.sh",
    "clean": "./scripts/clean.sh",
    "compile": "tsc --noEmit",
    "test": "vitest run",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "docgen": "typedoc",
    "deploy:docs": "gh-pages -d docs -e deposit-address"
  },
  "keywords": [],
  "author": "",
  "license": "LicenseRef-LICENSE",
  "devDependencies": {
    "@axelarjs/config": "workspace:*",
    "dotenv": "^16.4.5",
    "gh-pages": "^6.1.1",
    "rimraf": "^5.0.5",
    "typedoc": "^0.25.12",
    "typescript": "^5.4.3",
    "vitest": "1.2.1"
  },
  "dependencies": {
    "@axelarjs/api": "workspace:*",
    "@axelarjs/core": "workspace:*",
    "@axelarjs/cosmos": "workspace:*",
    "@axelarjs/proto": "workspace:*",
    "@axelarjs/utils": "workspace:*",
    "bech32": "^2.0.0",
    "viem": "^2.8.18"
  }
}
