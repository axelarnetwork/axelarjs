{
  "name": "axelarjs",
  "version": "0.1.0",
  "description": "Axelar Network foundations for javascript/Typescript based applications",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axelarnetwork/axelarjs"
  },
  "scripts": {
    "prepare": "husky install",
    "build": "turbo run build",
    "build:pkgs": "turbo run build --filter=\"./packages/*\"",
    "test": "turbo run test",
    "test:ci": "CI=true turbo run test",
    "lint": "turbo run lint",
    "compile": "turbo run compile",
    "format": "prettier --write .",
    "storybook": "pnpm -C ./packages/ui run storybook ",
    "build-storybook": "turbo run build-storybook",
    "docs:dev": "pnpm -C ./apps/docs dev",
    "e2e": "pnpm -C ./apps/e2e run test",
    "dev:maestro": "pnpm -C ./apps/maestro run dev",
    "dev:docs": "pnpm -C ./apps/docs run dev",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish",
    "cs": "pnpm changeset",
    "cs:version": "pnpm: changeset:version",
    "cs:publish": "pnpm changeset:publish"
  },
  "keywords": [
    "Axelar Network",
    "Cross-chain",
    "Interoperability"
  ],
  "author": "",
  "license": "LicenseRef-LICENSE",
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@ianvs/prettier-plugin-sort-imports": "^4.0.2",
    "eslint": "^8.42.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "turbo": "^1.10.2",
    "typescript": "^5.1.3"
  },
  "resolutions": {
    "@types/react": "18.2.5"
  }
}
